<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link rel="stylesheet" href="~/Scripts/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Scripts/css/bootstrap.min.css" />
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

</head>
<body>

  <div class="body-content" style="background-color: lightgray;">
    @RenderBody()
    <hr />
    <footer>
      <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>

    <script type="text/javascript" src="~/Scripts/Cleint-App/main.js"></script>
    <script type="text/javascript" src="~/Scripts/Cleint-App/polyfills.js"></script>
    <script type="text/javascript" src="~/Scripts/Cleint-App/runtime.js"></script>
    <script type="text/javascript" src="~/Scripts/Cleint-App/vendor.js"></script>
  </div>

</body>
</html>
